{
  "name": "chinayin/restapi-sdk",
  "description": "restapi sdk",
  "type": "library",
  "license": "Apache-2.0",
  "homepage": "http://github.com/chinayin/restapi-sdk",
  "keywords": [
    "restapi",
    "sdk"
  ],
  "authors": [
    {
      "name": "chinayin",
      "email": "whereismoney@qq.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phploc/phploc": "^5.0",
    "sebastian/phpcpd": "^4.0",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "RestAPI\\": "src/RestAPI/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RestAPI\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-ci": "vendor/bin/phpunit --coverage-text",
    "phploc": "vendor/bin/phploc --exclude=vendor --exclude=tests ./",
    "phpcpd": "vendor/bin/phpcpd --exclude=vendor --exclude=tests ./",
    "phpstan": "vendor/bin/phpstan analyse",
    "travis": [
      "composer phploc",
      "composer phpstan"
    ]
  }
}
